<%= form_for :student, url: teacher_students_path(@teacher_id) do |f| %>
 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :School_Id %><br>
    <%= f.number_field :number %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', teacher_students_path %>